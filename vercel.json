{
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/aggregate/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/aggregate",
      "destination": "/api/index"
    },
    {
      "source": "/",
      "destination": "/api/index"
    }
  ]
}
